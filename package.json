{
  "name": "memba-website",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:dev": "env-cmd -f ./env/.env.development next build",
    "build:prod": "env-cmd -f ./env/.env.production next build",
    "open-next-build:dev": "open-next build --build-command 'yarn build:dev'",
    "open-next-build:prod": "open-next build --build-command 'yarn build:prod'",
    "start": "next start",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint . --fix --ext ts,tsx",
    "prettier": "prettier \"./src/**/*.{ts,tsx,js,json}\" --check",
    "prettier:fix": "prettier \"./src/**/*.{ts,tsx,js,json}\" --check --write",
    "prepare": "husky install",
    "test": "jest src/app",
    "custom:build": "next build"
  },
  "dependencies": {
    "@aws-amplify/auth": "^5.4.0",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "axios": "^0.27.2",
    "encoding": "^0.1.13",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.6",
    "formik": "^2.4.2",
    "next": "13.4.6",
    "open-next": "^1.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^5.3.11",
    "swr": "^2.1.5",
    "typescript": "^4.9.4",
    "webpack": "^5.88.0",
    "yup": "^1.2.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@jamesacarr/eslint-formatter-github-actions": "^0.2.0",
    "@jest/types": "^29.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/styled-components": "^5.1.26",
    "@types/webpack": "^5.28.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.9",
    "amazon-cognito-identity-js": "^6.3.1",
    "aws-crt": "^1.15.18",
    "axios-mock-adapter": "^1.21.5",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
