    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-mock-axios": "^4.7.2",
    "jest-mock-extended": "^3.0.4",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8"
  }
